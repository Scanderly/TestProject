@using TestProject.Models
@{
    User user = Session["User"] as User;
}




@if (user != null)
{
    <a class="info btn btn-danger" href="@Url.Action("logout","login",new {Area="" })" role="button">Çıxış</a>
}
else
{
    <a class="info btn btn-info" href="@Url.Action("login","login",new {Area="" })" role="button">Daxil ol</a>
}


